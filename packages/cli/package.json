{
  "name": "@bronya.js/cli",
  "version": "0.11.2",
  "description": "type-safe cli builder inspired by cac",
  "license": "MIT",
  "homepage": "https://elysium-everlasting.github.io/bronya.js",
  "funding": "https://github.com/Elysium-Everlasting/bronya.js",
  "contributors": [
    {
      "name": "ap0nia",
      "email": "",
      "url": "https://github.com/ap0nia"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Elysium-Everlasting/bronya.js",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/Elysium-Everlasting/bronya.js/issues"
  },
  "keywords": [
    "command",
    "option",
    "parser",
    "cli",
    "argument",
    "args",
    "argv"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "prepublish": "tsup"
  },
  "dependencies": {},
  "devDependencies": {
    "@vitest/coverage-v8": "^0.34.0",
    "tsup": "^7.2.0",
    "tsx": "^3.12.7",
    "vitest": "^0.34.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "pnpm": "^8.0.0",
    "node": ">=18"
  }
}
