{
  "name": "@bronya.js/api-construct",
  "version": "0.8.0",
  "description": "api construct",
  "license": "MIT",
  "homepage": "https://elysium-everlasting.github.io/bronya.js",
  "funding": "https://github.com/Elysium-Everlasting/bronya.js",
  "contributors": [
    {
      "name": "ap0nia",
      "email": "",
      "url": "https://github.com/ap0nia"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Elysium-Everlasting/bronya.js",
    "directory": "packages/api-construct"
  },
  "bugs": {
    "url": "https://github.com/Elysium-Everlasting/bronya.js/issues"
  },
  "keywords": [
    "aws",
    "cdk"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./plugins/cli": {
      "types": "./dist/plugins/cli.d.ts",
      "import": "./dist/plugins/cli.js",
      "require": "./dist/plugins/cli.cjs",
      "default": "./dist/plugins/cli.js"
    },
    "./integrations/express": {
      "types": "./dist/integrations/express.d.ts",
      "import": "./dist/integrations/express.js",
      "require": "./dist/integrations/express.cjs",
      "default": "./dist/integrations/express.js"
    },
    "./integrations/lambda": {
      "types": "./dist/integrations/lambda.d.ts",
      "import": "./dist/integrations/lambda.js",
      "require": "./dist/integrations/lambda.cjs",
      "default": "./dist/integrations/lambda.js"
    }
  },
  "typesVersion": {
    "*": {
      ".": [
        "dist/index.d.ts"
      ],
      "./plugins/cli": [
        "dist/plugins/cli.d.ts"
      ],
      "./integrations/express": [
        "dist/integrations/express.d.ts"
      ],
      "./integrations/lambda": [
        "dist/integrations/lambda.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "prepublish": "tsup"
  },
  "dependencies": {
    "@bronya.js/cli": "workspace:^",
    "@bronya.js/core": "workspace:^",
    "acorn": "^8.10.0",
    "acorn-typescript": "^1.4.1",
    "aws-cdk-lib": "^2.92.0",
    "body-parser": "^1.20.2",
    "chokidar": "^3.5.3",
    "consola": "^3.2.3",
    "constructs": "^10.2.69",
    "cors": "^2.8.5",
    "defu": "^6.1.2",
    "express": "^4.18.2",
    "jiti": "^1.19.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/estree": "^1.0.1",
    "@types/express": "^4.17.17",
    "esbuild": "^0.19.2",
    "tsup": "^7.2.0",
    "tsx": "^3.12.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "pnpm": "^8.0.0",
    "node": ">=18"
  }
}
