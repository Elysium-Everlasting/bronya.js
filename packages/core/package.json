{
  "name": "@bronya.js/core",
  "version": "0.8.1",
  "description": "devOPs tool",
  "license": "MIT",
  "homepage": "https://elysium-everlasting.github.io/bronya.js",
  "funding": "https://github.com/Elysium-Everlasting/bronya.js",
  "contributors": [
    {
      "name": "ap0nia",
      "email": "",
      "url": "https://github.com/ap0nia"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Elysium-Everlasting/bronya.js",
    "directory": "packages/villv"
  },
  "bugs": {
    "url": "https://github.com/Elysium-Everlasting/bronya.js/issues"
  },
  "keywords": [
    "aws",
    "cdk"
  ],
  "type": "module",
  "bin": {
    "bunny": "dist/cli.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.js",
      "require": "./dist/api.cjs",
      "default": "./dist/api.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs",
      "default": "./dist/utils.js"
    }
  },
  "typesVersion": {
    "*": {
      ".": [
        "dist/index.d.ts"
      ],
      "./api": [
        "dist/api.d.ts"
      ],
      "./utils": [
        "dist/utils.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/cli.ts",
    "test": "vitest",
    "prepublish": "tsup"
  },
  "dependencies": {
    "@bronya.js/cli": "workspace:^",
    "acorn": "^8.10.0",
    "acorn-typescript": "^1.4.1",
    "aws-cdk-lib": "^2.92.0",
    "body-parser": "^1.20.2",
    "chokidar": "^3.5.3",
    "consola": "^3.2.3",
    "constructs": "^10.2.69",
    "cors": "^2.8.5",
    "defu": "^6.1.2",
    "express": "^4.18.2",
    "jiti": "^1.19.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/estree": "^1.0.1",
    "@types/express": "^4.17.17",
    "esbuild": "^0.19.2",
    "tsup": "^7.2.0",
    "tsx": "^3.12.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "pnpm": "^8.0.0",
    "node": ">=18"
  }
}
